{
    "name": "private-extension-manager-remote-helper",
    "displayName": "Private Extension Manager: Remote Helper",
    "description": "Lets Private Extension Manager work in remote workspaces.",
    "version": "1.5.0",
    "publisher": "garmin",
    "license": "Apache-2.0",
    "author": {
        "name": "Garmin International"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/joelspadin-garmin/vscode-private-extension-manager.git",
        "directory": "remote-helper"
    },
    "engines": {
        "vscode": "^1.51.0"
    },
    "extensionKind": [
        "ui"
    ],
    "icon": "media/icon.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "multi-root-ready",
        "remote"
    ],
    "activationEvents": [
        "onCommand:_privateExtensionManager.remoteHelper.getExtension",
        "onCommand:_privateExtensionManager.remoteHelper.getPlatform"
    ],
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run package",
        "clean": "rimraf ./dist ./out",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "test-compile": "tsc -p ./",
        "test-watch": "tsc -watch -p ./",
        "pretest": "npm run test-compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.19.6",
        "@types/source-map-support": "^0.5.3",
        "@types/vscode": "^1.51.0",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "eslint": "^7.13.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-import-resolver-typescript": "^2.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "glob": "^7.1.6",
        "mocha": "^8.2.1",
        "prettier": "^2.2.0",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "vsce": "^1.81.1",
        "vscode-test": "^1.4.1",
        "webpack": "^5.31.0",
        "webpack-cli": "^4.6.0"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "tabWidth": 4,
        "printWidth": 120,
        "endOfLine": "auto"
    }
}
